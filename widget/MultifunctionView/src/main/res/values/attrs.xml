<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--多功能view-->
    <declare-styleable name="mv_MultifunctionView">
        <attr name="mv_layout_type" format="integer" />
        <attr name="mv_line_visibility" format="boolean" />
        <attr name="mv_line_right_visibility" format="boolean" />
        <attr name="mv_image_visibility" format="boolean" />
        <attr name="mv_required_options_visibility" format="boolean" />
        <attr name="mv_left_text" format="string" />
        <attr name="mv_center_text" format="string" />
        <attr name="mv_center_bottom_text" format="string" />
        <attr name="mv_center_hint_text" format="string" />
        <attr name="mv_right_text" format="string" />
        <attr name="mv_left_color" format="color" />
        <attr name="mv_center_color" format="color" />
        <attr name="mv_center_hint_color" format="color" />
        <attr name="mv_left_size" format="dimension" />
        <attr name="mv_center_size" format="dimension" />
        <attr name="mv_center_hint_size" format="dimension" />
        <attr name="mv_left_icon" format="reference" />
        <attr name="mv_right_icon" format="reference" />
    </declare-styleable>
</resources>