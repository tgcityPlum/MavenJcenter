    <!--    application theme starting   -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/color_white</item>
        <item name="colorPrimaryDark">@color/color_white</item>
        <item name="colorAccent">@color/title_bag</item>
        <item name="toolbarStyle">@style/ClubToolbar</item>
        <item name="android:textColorPrimary">@color/color_context</item>
        <item name="android:textColorSecondary">@color/color_context</item>
        <item name="colorButtonNormal">@color/title_bag</item>
        <!--<item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>-->
    </style>

    <style name="AppBaseThemeWithWindowIsTranslucent" parent="AppBaseTheme">
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowDisablePreview">true</item>
    </style>

    <style name="LauncherTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@drawable/shape_solid_ffffff</item> <!-- 需要添加背景 -->
        <item name="android:windowFullscreen">true</item>
    </style>
    <!--    application theme ending   -->

    <!--    view style starting   -->
    <style name="ClubToolbar" parent="Widget.AppCompat.Toolbar">
        <!-- 设置该属性解决空白部分-->
        <item name="contentInsetStart">0dp</item>
    </style>

    <style name="TextAppearanceSize15Color666666" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">@dimen/sp_15</item>
        <item name="android:textColor">@color/color_666666</item>
    </style>

    <style name="CheckBox_Normal_999999_Activate_D31306" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/color_999999</item>
        <item name="colorControlActivated">@color/color_D31306</item>
    </style>

    <style name="CheckBox_Normal_999999_Activate_6B86FF" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/color_999999</item>
        <item name="colorControlActivated">@color/color_6B86FF</item>
    </style>
    <!--    view style ending   -->

    <!--Anim style starting -->
    <style name="AnimStyleEnterBottomExitBottom">
        <!-- 指定显示的动画xml -->
        <item name="android:windowEnterAnimation">@anim/in_from_bottom_duration_200</item>
        <!-- 指定消失的动画xml -->
        <item name="android:windowExitAnimation">@anim/out_to_bottom_duration_200</item>
    </style>

    <style name="AnimStyleEnterBottomExitBottomOpenClose" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/in_from_right_duration_200</item>
        <item name="android:activityOpenExitAnimation">@anim/out_to_left_duration_200</item>
        <item name="android:activityCloseEnterAnimation">@anim/in_from_right_close_duration_200
        </item>
        <item name="android:activityCloseExitAnimation">@anim/out_to_left_close_duration_200</item>
    </style>

    <style name="PageAnimalTheme" parent="@style/AppBaseThemeWithWindowIsTranslucent">
        <!--自定义页面切换动画-->
        <item name="android:windowAnimationStyle">@style/AnimStyleEnterBottomExitBottomOpenClose
        </item>
        <item name="android:windowActivityTransitions">true</item>
    </style>
    <!-- Anim style ending -->

